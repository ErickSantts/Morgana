
{
    "manifest_version": 2,
    "name": "Morgana", 
    "version" : "1.2",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["<all_urls>", "tabs", "webNavigation"],
    "browser_action":{
        "default_icon": "morgana.png",
        "default_title": "Click Para Ativar"
    }
}